<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.androidproject.hangman.ProfilePicActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="119dp"
        android:layout_height="107dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_man" />

    <Button
        android:id="@+id/button2"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/button3"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man2"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/button4"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man3"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/button5"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man4"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/button6"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        tools:layout_editor_absoluteX="289dp" />

    <Button
        android:id="@+id/button7"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_woman"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:layout_editor_absoluteX="48dp" />

    <Button
        android:id="@+id/button8"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:layout_editor_absoluteX="124dp" />

    <Button
        android:id="@+id/button9"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:layout_editor_absoluteX="208dp" />

    <Button
        android:id="@+id/button10"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man5"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/button11"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man6"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/button12"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man7"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/button13"
        style="@android:style/Widget.Material.Light.Button.Borderless"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_man8"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="300dp" />

</android.support.constraint.ConstraintLayout>
